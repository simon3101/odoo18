<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="module_category_view" model="ir.module.category">
        <field name="name">Views Chapter</field>
        <field name="sequence">41</field>
    </record>

    <record id="group_view_model_manager" model="res.groups">
        <field name="name">View manager</field>
        <field name="category_id" ref="module_category_view"/>
